package strings;

import java.util.Scanner;

public class SentenceCount {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String line = sc.nextLine();
		sc.close();
		line = line.trim();
		System.out.println((line.chars().filter(c -> c == 32).count()) + 1);
	}

}
