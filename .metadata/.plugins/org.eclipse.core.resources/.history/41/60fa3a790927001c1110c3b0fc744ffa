package strings;

import java.io.IOException;
import java.util.Scanner;

public class NumbersSum {
	
	public static void main(String[] args) throws IOException {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		sc.nextLine();
		int sum = 0;
		
		for(int i=0; i<n; i++) {
			int a = sc.nextInt();
			sum += a;
		}
		
		System.out.println(sum);
		
	}

}
